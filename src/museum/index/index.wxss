.card {
  width: 300px;
  height: 300px;
  background-color: pink;
  margin-bottom: 10px;
}
.kxhdalWrap {
  width: 100%;
  height: 350px;
}
.kxhdalWrap .listTitle {
  width: 100%;
  height: 50px;
  background-color: red;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.kxhdalWrap .hdalContent {
  width: 100%;
  height: 300px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.kjzxCard {
  width: 500px;
  height: 300px;
  background-color: palevioletred;
  line-height: 50px;
}
.museunm_wrap {
  width: 100%;
  overflow: hidden;
  background-color: #f8f8fa;
}
.museunm_wrap .breadcrumb {
  width: 1260px;
  margin: 20px auto -15px;
}
.museunm_wrap .commList {
  width: 1260px;
  margin: 0 auto;
}
.museunm_wrap .commList .listTitle {
  width: 100%;
  height: 50px;
  background-color: red;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.museunm_wrap .commList .listContent {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.museunm_wrap .kxhdalWrap {
  width: 1260px;
  margin: 0 auto 50px;
}
.museunm_wrap .xnkjg_comm {
  width: 406px;
  background: #fff;
  border-radius: 10px;
  overflow: hidden;
  display: block;
  text-decoration: none !important;
}
.museunm_wrap .xnkjg_comm .xnkjg_img {
  width: 100%;
  height: 260px;
  position: relative;
  display: block;
}
.museunm_wrap .xnkjg_comm .xnkjg_img .xnkjg_imgIcon {
  position: absolute;
  top: 20px;
  left: 0;
  width: 48px;
  height: 26px;
}
.museunm_wrap .xnkjg_comm .xnkjg_text {
  padding: 0 10px;
  padding-top: 15px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.museunm_wrap .xnkjg_comm .xnkjg_text .xnkjg_textL {
  font-size: 20px;
  font-family: "微软雅黑";
  font-weight: bold;
  color: #5b6571;
  cursor: pointer;
  flex: 1;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
.museunm_wrap .xnkjg_comm .xnkjg_text .xnkjg_textR {
  height: auto;
  line-height: 22px;
  font-size: 16px;
  font-family: "微软雅黑";
  color: #fff;
  display: flex;
  justify-content: center;
  padding: 0 7px;
  background: #007dde;
  border-radius: 5px;
  margin: 0 0 0 10px;
}
.museunm_wrap .xnkjg_comm .xnkjg_address {
  padding: 15px 10px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.museunm_wrap .xnkjg_comm .xnkjg_address .xnkjg_addressL {
  display: flex;
  align-items: center;
}
.museunm_wrap .xnkjg_comm .xnkjg_address .xnkjg_addressL img {
  width: 13px;
  height: 14px;
  padding-right: 5px;
}
.museunm_wrap .xnkjg_comm .xnkjg_address .xnkjg_addressL span {
  font-size: 14px;
  font-family: "微软雅黑";
  color: #818c99;
}
.museunm_wrap .xnkjg_comm .xnkjg_address .xnkjg_addressR {
  font-size: 14px;
  font-family: "微软雅黑";
  color: #818c99;
}
